    "color": {
        "1": "white"
    }
},

"modules": [
  
//System information

{
"type": "custom",
"format": "┌─────────────────System Information─────────────────┐"
},

{
    "type": "os",
    "keyColor": "{{colors.primary.default.hex}}"
},
{
    "type": "bios",
    "keyColor": "{{colors.primary.default.hex}}"
},
{
    "type": "kernel",
    "keyColor": "{{colors.primary.default.hex}}"
},
{    
    "type": "packages",
    "keyColor": "{{colors.secondary.default.hex}}"
},
{
    "type": "terminal",
        "keyColor": "{{colors.tertiary.default.hex}}"
},
{
    "type": "shell",
        "keyColor": "{{colors.tertiary.default.hex}}"
},
{    
    "type": "wm",
        "keyColor": "{{colors.tertiary.default.hex}}"
},
    
    {
"type": "custom",
"format": "└────────────────────────────────────────────────────┘"
},
    
    
//Hardware

{
"type": "custom",
"format": "┌──────────────────────Hardware──────────────────────┐"
},

{
    "type": "display",
     "keyColor": "{{colors.primary.default.hex}}"
},
{  
    "type": "cpu",
    "keyColor": "{{colors.primary.default.hex}}"
},
{
    "type": "gpu",
    "keyColor": "{{colors.secondary.default.hex}}"
},
{
    "type": "memory",
        "keyColor": "{{colors.tertiary.default.hex}}"
},
{
    "type": "swap",
        "keyColor": "{{colors.tertiary.default.hex}}"
},
{
    "type": "disk",
    "keyColor": "{{colors.tertiary.default.hex}}"
},
    
        {
"type": "custom",
"format": "└────────────────────────────────────────────────────┘"
},
     
//OS Age

     
{
"type": "custom",
"format": "┌───────────────────────OS Age───────────────────────┐"
},

{
    "type": "uptime",
    "keyColor": "{{colors.primary.default.hex}}"
},
{
"type": "command",
"key": "OS Age",
"keyColor": "{{colors.tertiary.default.hex}}",
"text": "birth_install=$(stat -c %W /); current=$(date +%s); time_progression=$((current - birth_install)); years=$((time_progression / (365*86400))); months=$(( (time_progression % (365*86400)) / (30*86400) )); days=$(( (time_progression % (30*86400)) / 86400 )); echo \"$days days, $months months, $years years\""
},

{
"type": "custom",
"format": "└────────────────────────────────────────────────────┘"
},
     
  ]

}
